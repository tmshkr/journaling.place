{
  "name": "journaling.place",
  "version": "0.4.2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:web": "turbo run dev --filter=web --filter=trpc-server",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:test": "docker compose -f docker-compose.test.yml up",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "prisma db push && prisma db seed && turbo run start --parallel",
    "e2e:test": "npm run test --workspace e2e",
    "e2e:report": "turbo run report --filter=e2e",
    "jest": "turbo run jest --filter=trpc-server"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.5.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.6",
    "@tanstack/react-query": "^5.4.3",
    "@trpc/client": "^10.43.1",
    "@trpc/server": "^10.43.1",
    "@types/jest": "^29.5.7",
    "@types/node": "20.8.10",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "@vercel/ncc": "^0.38.1",
    "agenda": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "cookie": "^0.5.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "email-templates": "^11.1.1",
    "eslint": "8.52.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "14.0.1",
    "express": "^4.18.2",
    "flexsearch": "^0.7.31",
    "jest-mock-extended": "^3.0.5",
    "localforage": "^1.10.0",
    "mongodb": "^6.2.0",
    "next": "^14.0.2",
    "next-auth": "^4.24.4",
    "next-connect": "^1.0.0",
    "next-seo": "^6.4.0",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "prettier": "latest",
    "pug": "^3.0.2",
    "quill": "^1.3.7",
    "quilljs-markdown": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-spinners": "^0.13.8",
    "react-tooltip": "^5.22.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "typescript": "5.2.2",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "prisma": "^5.5.2",
    "turbo": "1.10.16"
  },
  "prisma": {
    "seed": "node scripts/seed.mjs"
  },
  "optionalDependencies": {
    "@mermaid-js/mermaid-cli": "^10.6.0",
    "prisma-erd-generator": "^1.11.1"
  }
}
