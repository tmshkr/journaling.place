{
  "name": "journaling.place",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:web": "turbo run dev --filter=web --filter=trpc-server",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "start": "prisma db push && turbo run start --parallel",
    "e2e:test": "turbo run test --filter=e2e",
    "e2e:report": "turbo run report --filter=e2e",
    "jest": "prisma db push && prisma db seed && turbo run jest --filter=trpc-server",
    "jest:watch": "turbo run jest:watch --filter=web",
    "send:potd": "npm run send:potd --workspace mailer"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^9.2.2",
    "@playwright/test": "^1.30.0",
    "@svgr/webpack": "^6.5.1",
    "@types/jest": "^29.5.1",
    "autoprefixer": "^10.4.16",
    "eslint-config-custom": "*",
    "node-mocks-http": "^1.12.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "latest",
    "prisma": "^5.4.2",
    "prisma-erd-generator": "^1.2.4",
    "sass": "^1.69.4",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.0",
    "tsx": "^3.14.0",
    "turbo": "1.5.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@hookform/resolvers": "^2.9.10",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^5.4.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.40.0",
    "@trpc/server": "^10.40.0",
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "agenda": "^5.0.0",
    "axios": "^1.2.1",
    "babel-plugin-superjson-next": "^0.4.5",
    "clsx": "^1.2.1",
    "cookie": "^0.5.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "email-templates": "^11.0.3",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "express": "^4.18.2",
    "flexsearch": "^0.7.31",
    "localforage": "^1.10.0",
    "mongodb": "^4.13.0",
    "nanoid": "^4.0.0",
    "next": "^13.5.5",
    "next-auth": "^4.23.2",
    "next-connect": "^1.0.0-next.3",
    "next-seo": "^5.15.0",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.22",
    "pug": "^3.0.2",
    "quill": "^1.3.7",
    "quilljs-markdown": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.41.3",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-spinners": "^0.13.7",
    "react-tooltip": "^5.11.1",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "superjson": "^1.12.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4",
    "ws": "^8.13.0",
    "zod": "^3.22.4"
  },
  "packageManager": "npm@8.5.0",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
