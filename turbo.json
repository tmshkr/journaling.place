{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["package.json", "tsconfig.json"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "jest": {
      "cache": false
    },
    "jest:watch": {
      "cache": false
    },
    "report": {
      "cache": false
    }
  }
}
